<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.practicetest1">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN"
                    android:configChanges="orientation|screenSize" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ContextActivity" android:label="ContextActivity"/>
        <activity  android:name=".MediaActivity" android:label="MediaActivity"/>
        <activity  android:name=".ImplicitActivity" android:label="ImplicitActivity"/>
        <activity android:name=".SeekActivity" android:label="SeekActivity"/>
        <activity  android:name=".GallyActivity" android:label="GallyActivity" />
    </application>

</manifest>
